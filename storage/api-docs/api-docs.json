    "components": {
        "schemas": {
            "CreateAdminRequest": {
                "title": "Create Admin Request",
                "description": "Request payload for creating a new admin",
                "required": [
                    "first_name",
                    "last_name",
                    "email",
                    "password"
                ],
                "properties": {
                    "first_name": {
                        "type": "string",
                        "example": "John"
                    },
                    "last_name": {
                        "type": "string",
                        "example": "Doe"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "john@example.com"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "example": "password123"
                    },
                    "avatar": {
                        "type": "string",
                        "example": "avatar.jpg",
                        "nullable": true
                    },
                    "address": {
                        "type": "string",
                        "example": "123 Street, City",
                        "nullable": true
                    },
                    "phone_number": {
                        "type": "string",
                        "example": "123-456-7890",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "LoginAuthRequest": {
                "title": "Login Request",
                "description": "Login request payload",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "example": "user@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "password123"
                    }
                },
                "type": "object"
            },
            "CreateOrderRequest": {
                "title": "Create Order Request",
                "description": "Create Order Request body data",
                "required": [
                    "products",
                    "address",
                    "delivery_fee"
                ],
                "properties": {
                    "products": {
                        "description": "Array of products in the order",
                        "type": "array",
                        "items": {
                            "required": [
                                "product",
                                "quantity"
                            ],
                            "properties": {
                                "product": {
                                    "description": "UUID of the product",
                                    "type": "string",
                                    "format": "uuid",
                                    "example": "de305d54-75b4-431b-adb2-eb6b9e546014"
                                },
                                "quantity": {
                                    "description": "Quantity of the product in the order",
                                    "type": "integer",
                                    "example": 2
                                }
                            },
                            "type": "object"
                        }
                    },
                    "address": {
                        "description": "Address details",
                        "required": [
                            "billing",
                            "shipping"
                        ],
                        "properties": {
                            "billing": {
                                "description": "Billing address",
                                "type": "string"
                            },
                            "shipping": {
                                "description": "Shipping address",
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    "delivery_fee": {
                        "description": "Delivery fee for the order",
                        "type": "number",
                        "format": "float"
                    }
                },
                "type": "object"
            },
            "ListOrderRequest": {
                "title": "ListOrderRequest",
                "description": "List Order Request",
                "properties": {
                    "limit": {
                        "type": "integer",
                        "example": 10
                    },
                    "sort_by": {
                        "type": "string",
                        "enum": [
                            "uuid",
                            "user_id",
                            "order_status_id",
                            "products.*.product'"
                        ]
                    },
                    "desc": {
                        "type": "integer",
                        "enum": [
                            0,
                            1
                        ]
                    }
                },
                "type": "object"
            },
            "UpdateOrderRequest": {
                "title": "Update Order Request",
                "description": "Update Order Request body data",
                "required": [
                    "order_status_id",
                    "products",
                    "address",
                    "delivery_fee"
                ],
                "properties": {
                    "order_status_id": {
                        "description": "UUID of the order status for the order",
                        "type": "string",
                        "format": "uuid",
                        "example": "de305d54-75b4-431b-adb2-eb6b9e546014"
                    },
                    "products": {
                        "description": "Array of products in the order",
                        "type": "array",
                        "items": {
                            "required": [
                                "product",
                                "quantity"
                            ],
                            "properties": {
                                "product": {
                                    "description": "UUID of the product",
                                    "type": "string",
                                    "format": "uuid",
                                    "example": "de305d54-75b4-431b-adb2-eb6b9e546014"
                                },
                                "quantity": {
                                    "description": "Quantity of the product in the order",
                                    "type": "integer",
                                    "example": 2
                                }
                            },
                            "type": "object"
                        }
                    },
                    "address": {
                        "description": "Address details",
                        "properties": {
                            "billing": {
                                "description": "Billing address",
                                "type": "string"
                            },
                            "shipping": {
                                "description": "Shipping address",
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    "delivery_fee": {
                        "description": "Delivery fee for the order",
                        "type": "number",
                        "format": "float"
                    }
                },
                "type": "object"
            },
            "CreateProductRequest": {
                "title": "Create Product Request",
                "description": "Create Product Request Body",
                "required": [
                    "title",
                    "price",
                    "description",
                    "metadata"
                ],
                "properties": {
                    "title": {
                        "description": "Title of the product",
                        "type": "string",
                        "example": "Pet Food"
                    },
                    "price": {
                        "description": "Price of the product",
                        "type": "number",
                        "format": "float",
                        "example": 19.99
                    },
                    "description": {
                        "description": "Description of the product",
                        "type": "string",
                        "example": "High-quality pet food for all breeds."
                    },
                    "metadata": {
                        "description": "Metadata of the product",
                        "type": "object",
                        "example": {
                            "brand": "UUID from petshop.brands",
                            "image": "UUID from petshop.files"
                        }
                    }
                },
                "type": "object"
            },
            "ListProductRequest": {
                "title": "ListProductRequest",
                "description": "List Product Request",
                "properties": {
                    "limit": {
                        "type": "integer",
                        "example": 10
                    },
                    "sort_by": {
                        "type": "string",
                        "enum": [
                            "title",
                            "price",
                            "description"
                        ]
                    },
                    "desc": {
                        "type": "integer",
                        "enum": [
                            0,
                            1
                        ]
                    }
                },
                "type": "object"
            },
            "UpdateProductRequest": {
                "title": "UpdateProductRequest",
                "description": "Update Product Request",
                "required": [
                    "title",
                    "price",
                    "description",
                    "metadata"
                ],
                "properties": {
                    "title": {
                        "description": "Title of the product",
                        "type": "string",
                        "example": "Updated Product"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 39.99
                    },
                    "description": {
                        "type": "string",
                        "example": "This is the updated product description."
                    },
                    "metadata": {
                        "type": "object",
                        "example": {
                            "key": "updated_value"
                        }
                    }
                },
                "type": "object"
            },
            "Order": {
                "title": "Order",
                "description": "Order model",
                "required": [
                    "uuid",
                    "user_id",
                    "order_status_id",
                    "products",
                    "address",
                    "delivery_fee",
                    "amount",
                    "shipped_at"
                ],
                "properties": {
                    "uuid": {
                        "description": "UUID of the order",
                        "type": "string",
                        "format": "uuid",
                        "example": "de305d54-75b4-431b-adb2-eb6b9e546014"
                    },
                    "user_id": {
                        "description": "UUID of the user associated with the order",
                        "type": "string",
                        "format": "uuid",
